{
  "project": {
    "title": "Fortune Cookie Feedback System",
    "components": [
      {
        "id": "raspberry-pi-4",
        "type": "raspberry-pi-4",
        "position": {
          "x": 100,
          "y": 100
        }
      },
      {
        "id": "servo-motor",
        "type": "servo-motor",
        "position": {
          "x": 300,
          "y": 100
        },
        "connections": {
          "signal": "GPIO18",
          "vcc": "5V",
          "gnd": "GND"
        }
      },
      {
        "id": "cap-touch-sensor",
        "type": "cap-touch-sensor",
        "position": {
          "x": 100,
          "y": 300
        },
        "connections": {
          "sda": "GPIO2",
          "scl": "GPIO3",
          "vcc": "3.3V",
          "gnd": "GND"
        }
      }
    ],
    "wires": [
      {
        "id": "servo-signal",
        "from": {
          "component": "raspberry-pi-4",
          "pin": "GPIO18"
        },
        "to": {
          "component": "servo-motor",
          "pin": "signal"
        },
        "color": "#FFD700"
      },
      {
        "id": "servo-power",
        "from": {
          "component": "raspberry-pi-4",
          "pin": "5V"
        },
        "to": {
          "component": "servo-motor",
          "pin": "vcc"
        },
        "color": "#FF0000"
      },
      {
        "id": "servo-ground",
        "from": {
          "component": "raspberry-pi-4",
          "pin": "GND"
        },
        "to": {
          "component": "servo-motor",
          "pin": "gnd"
        },
        "color": "#000000"
      },
      {
        "id": "sensor-sda",
        "from": {
          "component": "raspberry-pi-4",
          "pin": "GPIO2"
        },
        "to": {
          "component": "cap-touch-sensor",
          "pin": "sda"
        },
        "color": "#00FF00"
      },
      {
        "id": "sensor-scl",
        "from": {
          "component": "raspberry-pi-4",
          "pin": "GPIO3"
        },
        "to": {
          "component": "cap-touch-sensor",
          "pin": "scl"
        },
        "color": "#0000FF"
      },
      {
        "id": "sensor-power",
        "from": {
          "component": "raspberry-pi-4",
          "pin": "3.3V"
        },
        "to": {
          "component": "cap-touch-sensor",
          "pin": "vcc"
        },
        "color": "#FF0000"
      },
      {
        "id": "sensor-ground",
        "from": {
          "component": "raspberry-pi-4",
          "pin": "GND"
        },
        "to": {
          "component": "cap-touch-sensor",
          "pin": "gnd"
        },
        "color": "#000000"
      }
    ]
  }
} 